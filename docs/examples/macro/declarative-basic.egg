pub macro decl-vec-type(coord-names: ![ident], coord-type: type) -> mod
return %%%
    pub struct Vec
        $$$
            %($name: $coord-type) for name in coord-names

    type RawVec = [type; $(length coord-names)]

    impl Convert(Vec, RawVec)
    with
        const fn convert(vec: Vec) -> RawVec
            [$(%(vec.$name) for name in coord-names)]
